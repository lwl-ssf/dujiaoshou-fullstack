<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dujiaoshou.mapper.OrderItemMapper">

  <insert id="insert" parameterType="com.example.dujiaoshou.entity.OrderItem" useGeneratedKeys="true" keyProperty="id">
    INSERT INTO order_items(order_id, product_id, quantity, unit_price)
    VALUES(#{order.id}, #{product.id}, #{quantity}, #{unitPrice})
  </insert>
</mapper>
